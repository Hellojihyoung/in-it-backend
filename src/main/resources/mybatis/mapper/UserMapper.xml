<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.init-backend.user.repository.UserRepository">
    <insert id="insertUser" parameterType="User">
        INSERT INTO USER (email, password, nickname, year, work_position)
        VALUES (#{email}, #{password}, #{nickname}, #{year} , #{work_position})
    </insert>
    <select id="findByEmail" parameterType="User">
        SELECT * FROM USER
        WHERE email = #{email}
    </select>
</mapper>